<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bryanrosales.zentre" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SingUp_Activity"
            android:label="@string/title_activity_sing_up_"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.bryanrosales.zentre.MainActivity" />
        </activity>
        <activity
            android:name=".Home_Activity"
            android:label="@string/title_activity_home_"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.bryanrosales.zentre.MainActivity" />
        </activity>

        <activity
            android:name=".User_Info_Activity"
            android:label="@string/title_activity_user_info_" >
        </activity>
        <activity
            android:name=".TabsActivity"
            android:label="@string/title_activity_tabs"
            android:icon="@drawable/user_icon">
        </activity>
        <activity
            android:name=".Dialog_Rest_1"
            android:label="@string/title_activity_dialog__rest_1" >
        </activity>

        <activity
            android:name=".Order_Activity"
            android:label="@string/title_activity_order_" >
        </activity>
    </application>

</manifest>
